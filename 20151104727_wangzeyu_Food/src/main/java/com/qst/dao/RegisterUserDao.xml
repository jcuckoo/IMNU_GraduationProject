<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qst.dao.RegisterUserDao">

		<insert id="RegisterByBean"
		parameterType="User">
		
		insert into elem_user(user_id,user_name,user_address,user_phone,user_pass)
		values(NULL,#{user_name},#{user_address},#{user_phone},#{user_pass})
		</insert>
		
		<insert id="updateByBean"
		parameterType="User">
		update elem_user set user_name=#{user_name},user_address=#{user_address},user_phone=#{user_phone}
		where user_id=#{user_id}
		</insert>
</mapper>